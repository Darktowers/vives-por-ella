@font-face 
    font-family: 'Gotham';
    src: url('../fonts/gotham-bold-webfont.woff2') format('woff2'),
         url('../fonts/gotham-bold-webfont.woff') format('woff')
    font-weight: bold;
    font-style: normal;
@font-face
    font-family: 'Gotham';
    src: url('../fonts/gotham-light-webfont.woff2') format('woff2'),
         url('../fonts/gotham-light-webfont.woff') format('woff')
    font-weight: normal;
    font-style: normal;
@import "nib"
@import "nib/vendor"
//estrellas
.back-Star
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: -1
    div
        position: fixed;
        border-radius: 50%;
.start0
    background: #FFFFFF;
.start1
    background: #fff;

.start2
    background: #fff;
.start3
    background-color: #fff;
.start4
    background-color: #fff;
.start5
    background-color: #fff;
.form
    input[type=text]
    input[type=email]
    input[type=password]
    textarea
    select
        color #1d1d1d
        outline-color: #03A9F4;
.form-Modal
    display: none;
    width: 100%;
    height: 100%;
    top: 0;
    position: fixed;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    z-index: 2;
    background: rgba(38, 50, 56, 0.63);
    font-size: 13px;
    .form
        max-width: 400px;
        width: 100%
        position: relative
        display: none;
        .close
            position: absolute;
            right: 0;
            font-size: 1.5em;
            padding: .8em;
            cursor: pointer
        form
            background: rgba(8, 26, 53, 0.8);
            padding: 3em 2em;
            padding-bottom: 0
            display: flex;
            justify-content: center;
            flex-direction: column;
            align-items: center;
            input[type=text]
            input[type=email]
            input[type=password]
            textarea
                width: 100%;
                padding: .5em;
                resize: none
            select
                padding: .5em;
            .fecha
                width: 100%;
            label
                width: 100%;
                padding: .5em 0;
            .enviar
                padding: 2em;
            input[type=submit]
                background: transparent;
                padding: 1em 2em;
                border: 1px solid;
                

//Main
body
    color: white;
    font-family: "Gotham";
    font-weight: normal;
    background: linear-gradient(180deg, #000 0%, #143043 50%, rgb(0, 48, 72) 75%);

.header
    &-Container
        margin: 0
    &-Menu
        display: flex;
        align-items: center;
        justify-content: center;
        padding: .5em 0;
        .menu
            position: absolute;
            left: 48px;
            transform: scale(1.3);
            cursor: pointer
        &-Nav
            position: fixed;
            left: 0;
            z-index: 2;
            height: 80%;
            top: 0;
            width: 100%;
            display: none;
            ul
                display: flex;
                flex-direction: column;
                padding: 0;
                margin: 0;
                padding-top: 4.7em;
                background: rgba(109, 110, 113, 0.8);
                width: 40%;
                height: 100%;
                min-width: 160px;
                li
                    display: flex;
                    a
                        padding: 1em;
                        text-align: left;
                        cursor: pointer;
                        width: 100%
                        transition: all .3s ease-in-out
                        &:hover
                            background: #585b5f
        .logo
            width: 150px;
            img
                width: 150px;
.footer
    padding-top: 6em;
    .container
        position: relative;
        .social 
            display: flex;
            justify-content: space-around;
            align-items: center;
            font-size: 1.6em;
            max-width: 215px;
            margin: 0 auto;
            padding-bottom: 1em;
        .site-Map
            p
                text-align: center
        .mundo
            img
                width: 100%
                position: relative;
                z-index: -2;
.modal-video
    display: flex;
    height: 100%;
    justify-content: center;
    align-items: center;
    display: none
    .video-cont
        width: 95%;
        height: 100%;
        max-height: 400px;
        max-width: 720px;
        iframe
            width: 100%;
            height: 100%;
.video
    max-width: 1000px;
    margin: 0 auto;
    &-Container
        iframe
            width: 100%
            display: none
    &-Back
        position: relative;
        img
            width: 100%
        .layout
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            display: flex;
            justify-content: center;
            align-items: center;
            z-index: 1;
            .play
                i
                    font-size: 6em;
                    color: rgba(8, 26, 53, 0.44);
                    cursor: pointer;
            .sound
                position: absolute;
                left: 0;
                top: 0;
                margin-left: .5em;
                margin-top: .5em;
                i
                    color: rgba(8, 26, 53, 0.44);
                    cursor: pointer;


    .social
        display: flex;
        justify-content: center;
        padding: 1em 0;
        a
            font-size: 1em;
            padding 0 1em;
    .text
        padding: 0 2em;
        h2
            font-size: 14px;
            font-weight: lighter;
        p
            font-size: 12px;
    .down
        display: flex;
        justify-content: center;
        padding: 2em 0;
.forms
    .forms-Info
        background: rgba(8,26,53,0.8);
        max-width: 250px;
        padding: 1em 2.5em;
        margin: 0 auto;
        text-align: center;
        margin-top: 1em;
        min-height: 200px;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        h3
            font-weight: lighter;
            font-size: 14px;
    .down
        display: flex;
        justify-content: center;
        padding: 2em 0;





